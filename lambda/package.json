{
  "name": "arpav-radar-veneto",
  "version": "1.2.0",
  "description": "alexa utility for quickly building skills",
  "main": "index.js",
  "scripts": {
    "ask-dialog": "cd .. && ask dialog",
    "build": "npm run clean && npm run compile && cp -r package.json documents ./dist && npm i --omit=dev --prefix dist",
    "clean": "rm -r dist || true",
    "compile": "tsc --build tsconfig.json",
    "deploy": "cd .. && npm run deploy",
    "start": "npm run build && npm run ask-dialog",
    "type-check": "tsc"
  },
  "author": "Amazon Alexa",
  "license": "Apache License",
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "dependencies": {
    "ask-sdk-core": "^2.14.0",
    "ask-sdk-model": "^1.86.0",
    "aws-sdk": "^2.1569.0",
    "cross-fetch": "^4.0.0",
    "he": "^1.2.0",
    "i18next": "^23.10.0",
    "typescript": "^5.3.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^20.1.1",
    "@tsconfig/recommended": "^1.0.3",
    "@tsconfig/strictest": "^2.0.3",
    "@types/he": "^1.2.3",
    "@types/xml2js": "^0.4.14",
    "ask-sdk-local-debug": "^1.1.0",
    "ts-node": "^10.9.2"
  }
}
